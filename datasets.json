{
  "presets": {
    "default": {
      "use_hint": true,
      "col_place": "city",
      "col_hint": "admin",
      "col_lon": "lng",
      "col_lat": "lat",
      "col_rank": "population",
      "col_group": "group",
      "file": "data/places.world.csv",
      "single_group": false,
      "harshness": 0.7,
      "default_level": 0,
      "filters": [],
      "levels": [
        {
          "label": "Facile",
          "weights": [1]
        },
        {
          "label": "Moyen",
          "weights": [1, 1]
        },
        {
          "label": "Difficile",
          "weights": [1, 1, 0.5]
        }
      ]
    },
    "continent": {
      "col_hint": "country_name",
      "label*": ["Normal", "Difficile", "Douloureux"],
      "weights*": [[1.5, 1], [1, 1, 1, 0.1], [1, 1, 1, 0.5]]
    },
    "country": {
      "levels": [
        {
          "label": "Facile",
          "weights": [1.2, 1.1, 1]
        },
        {
          "label": "Moyen",
          "weights": [1, 1, 1, 0.5]
        },
        {
          "label": "Difficile",
          "weights": [1, 1, 1, 1, 0.6]
        }
      ]
    }
  },
  "datasets": [
    {
      "group": null,
      "maps": [
        {
          "map_id": "world_capitals",
          "name": "Capitales du monde",
          "file": "data/capitals.world.csv",
          "col_lon": "lon",
          "col_hint": "country",
          "default_level": 2
        },
        {
          "name": "Villes du monde",
          "map_id": "world",
          "file": "data/places.world.csv",
          "scale": 0,
          "col_hint": "country_name"
        },
        {
          "map_id": "europe",
          "name": "Villes d'Europe",
          "spec_file": "data/continents/EUR.json",
          "preset": "continent",
          "col_hint": "country_name"
        },
        {
          "map_id": "france",
          "name": "Villes de France",
          "file": "data/places.france.csv",
          "use_hint": false,
          "default_level": 2,
          "levels": [
            {
              "label": "Facile",
              "weights": [1],
              "use_hint": true,
              "col_hint": "admin"
            },
            {"label":  "Normal", "weights": [1, 1,
              0.1]},
            {"label":  "Difficile", "weights":  [1, 1, 1]}
          ]
        },
        {
          "map_id": "XX",
          "name": "Evénements du XXème siècle",
          "file": "data/historic/XX.csv",
          "use_hint": false,
          "single_group": true
        }
      ]
    },
    {
      "group": "France",
      "maps": [
      {
        "map_id": "region_fr",
        "name": "Régions naturelles",
        "file": "data/france/france.region.csv",
        "use_hint": false,
        "harshness": 0.75,
        "single_group": true
      },
      {
        "map_id": "aoc_fr",
        "name": "Spécialités françaises",
        "file": "data/france/food.csv",
        "use_hint": false,
        "single_group": true
      },
      {
        "map_id": "cheese_fr",
        "name": "Fromages de France",
        "file": "data/france/cheese.csv",
        "use_hint": false,
        "single_group": true
      },
      {
        "map_id": "wine_fr",
        "name": "Vins de France",
        "file": "data/france/wine.csv",
        "use_hint": false,
        "single_group": true
      },
      {
        "map_id": "paris_landmarks",
        "name": "Monuments parisiens",
        "file": "data/cities/Paris.csv",
        "col_place": "name",
        "col_lon": "lon",
        "use_hint": false,
        "harshness": 0.25,
        "weights*": [
          [1], [1, 1], [1, 1, 0.6]
        ]
      },
      {
        "map_id": "paris_subway",
        "name": "Métro parisien",
        "file": "data/france/metro.paris.csv",
        "col_place": "place",
        "col_hint":  "ligne",
        "col_lon": "lon",
        "col_rank": "transit",
        "use_hint": true,
        "harshness": 0.3,
        "weights*": [
          [1], [1, 1], [1, 1, 1]
        ],
        "default_level": 2,
        "label*": ["Facile", "Normal", "Difficile"]
      }
      ]
    },
    {
    "group": "Capitales",
    "maps": [
       {
        "map_id": "world_g",
        "name": "Capitales du monde",
        "file": "data/capitals.world.csv",
        "col_lon": "lon",
        "col_hint": "country",
        "col_place": "city",
        "default_level": 2
      },
      {
        "map_id": "continent_<id>",
        "name": "Capitales d'<name>",
        "file": "data/capitals/*.csv",
        "col_lon": "lon",
        "col_hint": "country",
        "col_place": "city",
        "single_group": true
      }
    ]
  },
    {
    "group": "Par continent",
    "maps": [
      {
        "map_id": "<id>_grouped",
        "name": "<name>",
        "spec_file": "data/continents/*.json",
        "file": "data/places.world.csv",
        "preset": "continent",
        "col_hint": "country_name"
      }
    ]
  },
    {
      "group": "Par pays",
      "maps": [
        {
          "method": "generate_countries",
          "args": {
            "base_file": "data/places.world.csv",
            "column": "iso2",
            "preset": "country"
          }
        }
      ]
    },
  {
    "group": "Par ville",
    "maps": [
      {
        "map_id": "<id>",
        "name": "<id>",
        "file": "data/cities/*.csv",
        "col_place": "name",
        "col_lon": "lon",
        "use_hint": false,
        "single_group": true
      }
    ]
  },
    {
      "group": "Test",
      "maps": [
        {
        "map_id": "france_pref",
        "name": "Préfectures",
        "file": "data/misc/france.presfo.csv",
        "col_place": "city",
        "col_hint": "altname",
        "col_lon": "lon",
        "use_hint": true
      },
      {
        "map_id": "france_subpref",
        "name": "Préf. et sous-préfectures",
        "file": "data/misc/france.prefs.csv",
        "col_place": "city",
        "col_hint": "altname",
        "col_lon":  "lon",
        "use_hint": true},
        {
        "map_id": "france_subpref_code",
        "name": "Préf. et sous-préfectures",
        "file": "data/misc/france.prefs.csv",
        "col_place": "city",
        "col_hint": "dcode",
        "col_lon": "lon",
        "use_hint": true
      }
      ]
    }
  ]
}